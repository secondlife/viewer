			<button label="Fechar" name="close"/>
		</layout_panel>
		<button name="seek" label="avançar"/>
		<button name="rewind" label="voltar"/>
		<button name="stop" label="parar"/>
	</layout_stack>
	<layout_panel name="time_controls"/>
</floater>
