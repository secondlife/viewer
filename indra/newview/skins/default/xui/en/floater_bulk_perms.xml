    <button
      height="23"
    layout="topleft"
   name="check_all"
     label="âˆš All"
     left="290"
     top="26"
     width="115">
      <button.commit_callback
       function="BulkPermission.CheckAll" />
    </button>
    <button
     follows="left|top"
     height="23"
     label="Clear"
     label_selected="None"
     layout="topleft"
     top_pad="8"
     name="check_none"
     width="115" >
