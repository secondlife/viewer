	<widgetset name="web">
		<label name="web_label">
			Web Content
		</label>
		<icon>
			icn_media_web.tga
		</icon>
		<default_type>
			text/html
		</default_type>
		<tooltip name="web_tooltip">
			This location has Web content
		</tooltip>
		<playtip name="web_playtip">
			Show Web content
		</playtip>
		<allow_resize>
			true
		</allow_resize>
		<allow_looping>
			false
		</allow_looping>
	</widgetset>
