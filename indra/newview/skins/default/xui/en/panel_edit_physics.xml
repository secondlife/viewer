				top_pad="20"
				width="303" />
		</accordion_tab>
		<accordion_tab
			layout="topleft"
			fit_panel="false"
			min_height="50"
			name="physics_advanced_tab"
			title="Advanced Parameters">
			<scrolling_panel_list
				follows="all"
				layout="topleft"
				left="0"
				name="physics_advanced_param_list"
				top_pad="10"
				width="303" />
		</accordion_tab>
	</accordion>
    </panel>
</panel>

