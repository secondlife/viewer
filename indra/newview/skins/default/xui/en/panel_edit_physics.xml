			<scrolling_panel_list
				follows="all"
				layout="topleft"
				left="0"
				name="physics_breasts_inout_param_list"
				top_pad="50"
				width="303" />
		</accordion_tab>
		<accordion_tab
			layout="topleft"
			fit_panel="false"
			min_height="50"
			name="physics_breasts_leftright_tab"
