     label="Touch"
     layout="topleft"
     name="touch_attach">
        <on_click
         function="Attachment.Touch" />
        <on_enable
         function="Attachment.OnEnable"
         parameter="touch" />
    </menu_item_call>
    <menu_item_call
     label="Edit"
     layout="topleft"
     name="edit_item">
        <on_click
         function="Attachment.Edit" />
        <on_enable
         function="Attachment.OnEnable"
         parameter="edit" />
   </menu_item_call>
    <menu_item_call
     label="Detach"
     layout="topleft"
     name="detach">
        <on_click
         function="Attachment.Detach"
         parameter="detach"/>
    </menu_item_call>
</context_menu>
