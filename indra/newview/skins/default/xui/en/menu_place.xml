<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<toggleable_menu
 height="201"
 layout="topleft"
 mouse_opaque="false"
 name="place_overflow_menu"
 width="128">
     <menu_item_call
     label="Make a Landmark"
     layout="topleft"
     name="landmark">
        <menu_item_call.on_click
         function="Places.OverflowMenu.Action"
         parameter="landmark" />
    </menu_item_call>
    <menu_item_call
     label="Create Pick"
     layout="topleft"
     name="pick">
        <menu_item_call.on_click
         function="Places.OverflowMenu.Action"
         parameter="pick" />
        <menu_item_call.on_enable
         function="Places.OverflowMenu.Enable"
         parameter="can_create_pick" />
    </menu_item_call>
</toggleable_menu>
