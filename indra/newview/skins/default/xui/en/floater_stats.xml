				 bar_min="0.f"
				 bar_max="400.f" 
				 tick_spacing="100.f"
				 label_spacing="200.f" 
				 precision="1"
				 show_per_sec="false" 
         show_bar="false">
        </stat_bar>
			</stat_view>
        <!--Network Stats-->
			<stat_view
