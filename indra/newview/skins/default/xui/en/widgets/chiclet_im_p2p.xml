     left="25"
     mouse_opaque="false"
     name="unread"
     text_color="white"
     v_pad="3"
     visible="false"
     width="20"/>
    <chiclet_im_p2p.new_message_icon
  bottom="11"
  height="14"
  image_name="Unread_Chiclet"
  left="12"
  name="new_message_icon"
