     height="23"
     image_selected="PushButton_Selected"
     image_unselected="PushButton_Off"
     name="chiclet_button"
     tab_stop="false"
     width="25"/>
    <chiclet_im_p2p.speaker
     auto_update="true"
     draw_border="false"
     height="23"
     left="25"
     name="speaker"
