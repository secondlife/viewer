     height="23"
     image_selected="PushButton_Selected"
     image_unselected="PushButton_Off"
     name="chiclet_button"
     tab_stop="false"
     width="25"/>
    <chiclet_im_adhoc.speaker
     auto_update="true"
     draw_border="false"
     height="23"
     left="25"
     name="speaker"
     visible="false"
     width="20" />
    <chiclet_im_adhoc.avatar_icon
     bottom="2"
     follows="left|top|bottom"
     height="19"
     left="3"
     mouse_opaque="true"
     name="adhoc_icon"
     width="18" />
    <chiclet_im_adhoc.unread_notifications
     halign="center"
     height="23"
     left="25"
     mouse_opaque="false"
     name="unread"
     text_color="white"
     v_pad="3"
     visible="false"
     width="20" />
    <chiclet_im_adhoc.new_message_icon
  bottom="11"
  height="14"
  image_name="Unread_Chiclet"
  left="12"
  name="new_message_icon"
  visible="false"
  width="14" />
</chiclet_im_adhoc>