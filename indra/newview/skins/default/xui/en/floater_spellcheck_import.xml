   top_pad="10"
   left="2"
   height="0"
   width="396"
   follows="left|top"
   bevel_style="none"
   border_thickness="1"
   mouse_opaque="false"
   name="divisor"/>
  <button
   top_pad="10"
   right="280"
   height="22"
   width="90"
   enabled="true"
   follows="left|top"
   mouse_opaque="true"
   halign="center"
   scale_image="true"
   name="ok_btn"
   label="Import" />
  <button
   top_delta="0"
   right="380"
   height="22"
   width="90"
   enabled="true"
   follows="left|top"
   mouse_opaque="true"
   halign="center"
   scale_image="true"
   name="cancel_btn"
   label="Cancel" />
</floater>
