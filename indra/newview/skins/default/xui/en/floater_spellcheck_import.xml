<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater
 border="true"
 can_close="true"
 can_minimize="true"
 bottom="275"
 left="300"
 can_resize="false"
 height="140"
 width="400"
 name="spellcheck_import"
 title="Import Dictionary">
