<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
   background_visible="true"
   bg_alpha_color="InventoryBackgroundColor"
   border="false"
   follows="all"
   height="390"
   name="Inventory Gallery"
   layout="topleft">
  <text
    type="string"
    clip_partial="false"
    follows="all"
    layout="topleft"
    name="empty_txt"
    height="390"
    halign="center"
    valign="center"
    parse_urls="true"
    wrap="true">
    Folder is empty.
  </text> 
  <scroll_container
   follows="all"
   height="390"
   layout="topleft"
   left="0"
   top="0"
   tab_stop="true"
   name="gallery_scroll_panel"
   opaque="false">
  </scroll_container>
</panel>
