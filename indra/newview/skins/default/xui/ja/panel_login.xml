<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel name="panel_login">
	<text name="first_name_text" left="20">
		ファーストネーム：
	</text>
	<line_editor left="20" name="first_name_edit" width="126" />
	<text name="last_name_text" left="158">
