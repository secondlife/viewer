<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<panel label="水面" name="panel_settings_water">
	<layout_stack name="water_stack1" follows="all" layout="topleft" left="5" top="5" right="-5" bottom="-5" orientation="vertical">
		<layout_panel name="lp_top">
			<text name="water_fog_label">
				水中のフォグ:
			</text>
			<text name="color_label">
				色:
			</text>
			<text name="density_exponent_label">
				密度指数:
			</text>
			<slider initial_value="0" name="water_fog_density"/>
			<text name="underwater_modifier_label">
				水中の修飾子:
			</text>
			<slider initial_value="0" name="water_underwater_mod"/>
			<text name="FresnelScaleText">
				フレネル係数:
			</text>
			<slider initial_value="0.7" name="water_fresnel_scale"/>
			<text name="FresnelOffsetText">
				フレネル・オフセット:
			</text>
			<slider initial_value="0.7" name="water_fresnel_offset"/>
		</layout_panel>
		<layout_panel name="lp_bottom">
			<layout_stack name="water_stack2">
				<layout_panel name="lp_left">
					<text name="normal_map_label">
						ノーマル・マップ
					</text>
					<text name="large_wave_speed_label">
						大波の速度
					</text>
					<text name="small_wave_speed_label">
						小波の速度
					</text>
					<text name="reflection_wavelet_scale_label">
						さざ波の反射係数
					</text>
					<slider initial_value="0.7" label="X:" name="water_normal_scale_x"/>
					<slider initial_value="0.7" name="water_normal_scale_y" label="Y:"/>
					<slider initial_value="0.7" name="water_normal_scale_z" label="Z:"/>
				</layout_panel>
				<layout_panel name="lp_right">
					<text name="refraction_and_blur_label">
						反射とブラー:
					</text>
					<text name="refraction_scale_above_label">
						反射係数(上)
					</text>
					<slider initial_value="0.1" name="water_scale_above"/>
					<text name="refraction_scale_below_label">
						反射係数(下)
					</text>
					<slider initial_value="0" name="water_scale_below"/>
					<text name="blur_multiplier_label">
						ブラー乗数
					</text>
					<slider initial_value="0" name="water_blur_multip"/>
				</layout_panel>
			</layout_stack>
		</layout_panel>
	</layout_stack>
</panel>
