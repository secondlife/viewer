<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<panel label="雲" name="panel_settings_sky_clouds">
	<layout_stack name="main_ls">
		<layout_panel name="left_lp">
			<text name="cloud_color_label">
				雲の色:
			</text>
			<text name="cloud_coverage_label">
				雲の量:
			</text>
			<slider initial_value="0" name="cloud_coverage"/>
			<text name="cloud_scale_label">
				雲のスケール:
			</text>
			<slider initial_value="0" name="cloud_scale"/>
			<text name="cloud_variance_label">
				雲の分散:
			</text>
			<slider initial_value="0" name="cloud_variance"/>
			<text name="cloud_scroll_label">
				雲の移動:
			</text>
			<text name="cloud_image_label">
				雲の画像:
			</text>
		</layout_panel>
		<layout_panel name="right_lp">
			<text name="cloud_density_label">
				雲の密度:
			</text>
			<slider label="x" initial_value="0" name="cloud_density_x"/>
			<slider label="Y" initial_value="0" name="cloud_density_y"/>
			<slider label="D" initial_value="0" name="cloud_density_d"/>
			<text name="cloud_detail_label">
				雲の詳細:
			</text>
			<slider label="x" initial_value="0" name="cloud_detail_x"/>
			<slider label="Y" initial_value="0" name="cloud_detail_y"/>
			<slider label="D" initial_value="0" name="cloud_detail_d"/>
		</layout_panel>
	</layout_stack>
</panel>
