<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<panel label="密度" name="panel_settings_sky_density">
	<layout_stack name="main_ls">
		<layout_panel name="lp_1">
			<slider initial_value="0" name="rayleigh_exponential" label="レイリー散乱 指数項:"/>
			<slider initial_value="0" name="rayleigh_exponential_scale" label="レイリー散乱 指数係数："/>
			<slider initial_value="0" name="rayleigh_linear" label="レイリー散乱 線形項:"/>
			<slider initial_value="0" name="rayleigh_constant" label="レイリー散乱 定数項:"/>
			<slider initial_value="0" name="rayleigh_max_altitude" label="レイリー最大高度:"/>
		</layout_panel>
		<layout_panel name="lp_2">
			<slider initial_value="0" name="mie_exponential" label="ミー散乱 指数項:"/>
			<slider initial_value="0" name="mie_exponential_scale" label="ミー散乱 指数係数:"/>
			<slider initial_value="0" name="mie_linear" label="ミー散乱 線形項:"/>
			<slider initial_value="0" name="mie_constant" label="ミー散乱 定数項:"/>
