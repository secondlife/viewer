			<slider initial_value="0" name="mie_max_altitude" label="ミー散乱 最大高度:"/>
		</layout_panel>
		<layout_panel name="lp_3">
			<slider initial_value="0" name="absorption_exponential" label="光の吸収指数項:"/>
			<slider initial_value="0" name="absorption_exponential_scale" label="光の吸収の指数係数:"/>
			<slider initial_value="0" name="absorption_linear" label="吸収線形項:"/>
			<slider initial_value="0" name="absorption_constant" label="吸収定数項:"/>
			<slider initial_value="0" name="absorption_max_altitude" label="光の吸収線形項:"/>
		</layout_panel>
	</layout_stack>
</panel>
